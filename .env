# 应用配置
VUE_APP_NAME=Multi Social Platform
VUE_APP_VERSION=1.0.0
VUE_APP_DESCRIPTION=多平台社交管理器

# 构建环境
NODE_ENV=development
VUE_APP_ENV=development

# API配置
VUE_APP_API_BASE_URL=https://api.example.com
VUE_APP_API_TIMEOUT=10000

# 翻译服务配置
VUE_APP_GOOGLE_TRANSLATE_API_KEY=your_google_translate_api_key_here
VUE_APP_BAIDU_TRANSLATE_APP_ID=your_baidu_app_id_here
VUE_APP_BAIDU_TRANSLATE_SECRET=your_baidu_secret_here
VUE_APP_YOUDAO_TRANSLATE_APP_KEY=your_youdao_app_key_here
VUE_APP_YOUDAO_TRANSLATE_SECRET=your_youdao_secret_here
VUE_APP_TENCENT_TRANSLATE_SECRET_ID=your_tencent_secret_id_here
VUE_APP_TENCENT_TRANSLATE_SECRET_KEY=your_tencent_secret_key_here

# 错误上报配置
VUE_APP_SENTRY_DSN=your_sentry_dsn_here
VUE_APP_ERROR_REPORT_ENDPOINT=https://api.example.com/errors

# 更新服务配置
VUE_APP_UPDATE_SERVER_URL=https://update.example.com
VUE_APP_UPDATE_CHECK_INTERVAL=3600000

# 应用特性开关
VUE_APP_ENABLE_ANALYTICS=false
VUE_APP_ENABLE_CRASH_REPORT=true
VUE_APP_ENABLE_AUTO_UPDATE=true
VUE_APP_ENABLE_TELEMETRY=false

# 开发调试
VUE_APP_DEBUG_MODE=false
VUE_APP_LOG_LEVEL=info
VUE_APP_ENABLE_MOCK=false

# Electron配置
VUE_APP_ELECTRON_DEV_TOOLS=true
VUE_APP_ELECTRON_SECURITY=true

# 代理配置（开发环境）
VUE_APP_PROXY_HOST=127.0.0.1
VUE_APP_PROXY_PORT=8080

# 外部服务配置
VUE_APP_GITHUB_REPO=https://github.com/username/multi-social-platform
VUE_APP_DOCUMENTATION_URL=https://docs.example.com
VUE_APP_SUPPORT_EMAIL=support@example.com

# 安全配置
VUE_APP_ENCRYPTION_KEY=your_encryption_key_here
VUE_APP_SALT=your_salt_here

# 通知配置
VUE_APP_NOTIFICATION_ICON=/icons/notification.png
VUE_APP_NOTIFICATION_SOUND=/sounds/notification.mp3

# 性能配置
VUE_APP_MAX_CONTAINERS=20
VUE_APP_MAX_MEMORY_USAGE=2048
VUE_APP_CLEANUP_INTERVAL=300000

# 数据存储配置
VUE_APP_DATA_DIR=./data
VUE_APP_CACHE_DIR=./cache
VUE_APP_LOG_DIR=./logs

# 网络配置
VUE_APP_REQUEST_TIMEOUT=30000
VUE_APP_MAX_RETRIES=3
VUE_APP_RETRY_DELAY=1000

# UI配置
VUE_APP_DEFAULT_THEME=light
VUE_APP_DEFAULT_LANGUAGE=zh-CN
VUE_APP_ANIMATION_DURATION=300

# 浏览器指纹配置
VUE_APP_DEFAULT_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36
VUE_APP_DEFAULT_SCREEN_RESOLUTION=1920x1080
VUE_APP_DEFAULT_TIMEZONE=Asia/Shanghai

# 平台配置
VUE_APP_SUPPORTED_PLATFORMS=whatsapp,telegram,wechat,line,discord,slack,teams,skype,messenger,instagram
VUE_APP_PLATFORM_ICONS_PATH=/icons/platforms/
VUE_APP_PLATFORM_CONFIGS_PATH=/configs/platforms/

# 开发者配置
VUE_APP_DEVELOPER_NAME=Your Name
VUE_APP_DEVELOPER_EMAIL=developer@example.com
VUE_APP_DEVELOPER_GITHUB=https://github.com/developer
VUE_APP_LICENSE=MIT

# 分析和监控
VUE_APP_GOOGLE_ANALYTICS_ID=your_ga_id_here
VUE_APP_MIXPANEL_TOKEN=your_mixpanel_token_here
VUE_APP_HOTJAR_ID=your_hotjar_id_here

# CDN配置
VUE_APP_CDN_BASE_URL=https://cdn.example.com
VUE_APP_ASSETS_CDN_URL=https://assets.example.com

# 备份配置
VUE_APP_BACKUP_INTERVAL=86400000
VUE_APP_MAX_BACKUP_FILES=10
VUE_APP_BACKUP_ENCRYPTION=true

# 实验性功能
VUE_APP_ENABLE_WEBRTC=false
VUE_APP_ENABLE_SCREEN_SHARE=false
VUE_APP_ENABLE_FILE_SYNC=false
VUE_APP_ENABLE_CLOUD_BACKUP=false

# 合规性配置
VUE_APP_PRIVACY_POLICY_URL=https://example.com/privacy
VUE_APP_TERMS_OF_SERVICE_URL=https://example.com/terms
VUE_APP_GDPR_COMPLIANT=true
VUE_APP_CCPA_COMPLIANT=true